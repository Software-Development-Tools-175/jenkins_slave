import groovy.lang.Binding

def file = new File('args.csv')
def rows = file.readLines().tail()*.split(',')
assert file.readLines()*.split(',') == data

pipeline {
    agent any

    stages {

        stage('Welcome to main branch'){
            steps{
                echo 'Start Program'
            }
        }

        stage('Pre-build') {
            steps {
                sh 'echo Pre-build'
                sh 'python3 --version'
            }
        }

        stage('Parse value from CSV'){
            steps{
                echo "records ${rows}"
            }
        }

        stage('Trigger the Slave Jobs') {
            steps {
                echo 'Start Trigger to send name and age to slave'
                echo 'Triggering ...'
                build job: 'slave', parameters: [string(name: 'NAME', value: 'Alice'), string(name: 'Ages', value: '21')]
            }
        }
    }
}
