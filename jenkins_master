def mapper = new CsvMapper().readerForListOf(String).with(CsvParser.Feature.WRAP_AS_ARRAY)
file.withReader { r ->
    assert mapper.readValues(r).readAll() == data
}

pipeline {
    agent any

    stages {

        stage('Welcome to main branch'){
            steps{
                echo 'Start Program'
            }
        }

        stage('Pre-build') {
            steps {
                sh 'echo Pre-build'
                sh 'python3 --version'
            }
        }

        stage('Parse value from CSV'){
            steps{
                echo "records ${mapper}"
            }
        }

        stage('Trigger the Slave Jobs') {
            steps {
                echo 'Start Trigger to send name and age to slave'
                echo 'Triggering ...'
                build job: 'slave', parameters: [string(name: 'NAME', value: 'Alice'), string(name: 'Ages', value: '21')]
            }
        }
    }
}
