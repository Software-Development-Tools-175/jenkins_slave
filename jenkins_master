pipeline {
    agent any

    stages {

        stage('Welcome to main branch'){
            steps{
                echo 'Start Program'
            }
        }

        stage('Pre-build') {
            steps {
                sh 'echo Pre-build'
                sh 'python3 --version'
                echo "reaching CSV file..."
                sh "python parsingCSV.py"
            }
        }

        stage('Parse value from CSV'){
            steps{
                script{
                    try{
                        env.member = sh(script: "python parsingCSV.py",returnStdout: true)
                    }catch(Exception e){
                        echo "Error occured"
                    }
                }
            }
        }

        stage('Trigger the Slave Jobs') {
            steps {
                echo 'Start Trigger to send name and age to slave'
                echo 'Triggering ...'
                build job: 'slave', parameters: [string(name: 'memberList', value: "$member")]
            }
        }
    }
}
