pipeline {
    agent any

    parameters {
        string(name: 'NAME', defaultValue: 'Alice')
        string(name: 'Ages', defaultValue: '21')
    }

    stages {
        stage('build') {
            steps {
              echo 'build in progress'
              sh ('python myscript.py ' + params.NAME + " --age " + params.Ages)
            }
        }
    }

}
